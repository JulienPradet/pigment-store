{
  "name": "occitest",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist && cp src/generator/html/index.html dist/generator/html/index.html",
    "serve": "node --use-strict dist/generator/index.js",
    "build:watch": "babel -w src -d dist",
    "serve:watch": "nodemon --watch dist dist/generator/index.js",
    "prestart": "npm run build",
    "start": "npm run serve",
    "dev": "parallelshell \"npm run build:watch\" \"npm run serve:watch\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browserify-middleware": "^7.0.0",
    "data.task": "^3.1.0",
    "express": "^4.13.4",
    "mkdirp": "^0.5.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-router": "^2.4.0",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "private": true
}
